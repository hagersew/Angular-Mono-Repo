{
  "name": "angularmonorepo",
  "version": "1.0.0",
  "license": "",
  "description": "",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "prestart": "npm run tailwind:dev",
    "start": "ng serve",
    "tailwind:dev": "postcss styles/tailwind.css -o styles/tailwind-build.dev.css",
    "tailwind:prod": "postcss styles/tailwind.css -o styles/tailwind-build.prod.css",
    "prebuild": "cross-env NODE_ENV=production  npm run tailwind:prod",
    "build": "ng build",
    "build:cyber:qa": "ng run cyber:build:qa",
    "build:cyber:production": "ng run cyber:build:production",
    "build:cyber:dev": "ng run cyber:build:dev",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "mock:server": "json-server --watch mocks/data.json --routes routes.json",
    "start:proxy": "ng serve --proxy-config proxy.conf.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.0",
    "@angular/common": "^10.0.0",
    "@angular/compiler": "^10.0.0",
    "@angular/core": "^10.0.0",
    "@angular/forms": "^10.0.0",
    "@angular/platform-browser": "^10.0.0",
    "@angular/platform-browser-dynamic": "^10.0.0",
    "@angular/router": "^10.0.0",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@ngxs/router-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "date-fns": "^2.16.1",
    "intl-tel-input": "^17.0.8",
    "localforage": "^1.9.0",
    "lodash-es": "^4.17.15",
    "ng-zorro-antd": "^9.3.0",
    "ngx-permissions": "^8.1.1",
    "ngx-pipes": "^2.7.5",
    "rxjs": "~6.5.5",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "^10.0.0",
    "@angular/language-service": "^10.0.0",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@nrwl/angular": "^10.0.12",
    "@nrwl/workspace": "10.0.12",
    "@types/node": "~8.9.4",
    "angular-tslint-rules": "^1.20.4",
    "codelyzer": "~5.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "husky": "^4.2.5",
    "json-server": "^0.16.1",
    "postcss-cli": "^7.1.1",
    "prettier": "2.0.4",
    "tailwindcss": "^1.7.2",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.9.3"
  }
}
